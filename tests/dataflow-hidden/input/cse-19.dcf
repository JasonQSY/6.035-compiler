callout printf;
int get_int ( int x ) {
  return x;
}
boolean get_bool ( boolean x ) {
  return x;
}
void main ( ) {
  int a, b, c, d, e;
  boolean x;
  a = get_int ( 7 );
  b = get_int ( 4 );
  x = get_bool ( true );
  c = 0;
  d = 0;
  e = 0;
  c = a % b;
  if ( x ) {
    d = a % b;
  }
  else {
    e = a % b;
  }
  printf ( "%d\n", c );
  printf ( "%d\n", d );
  printf ( "%d\n", e );
}
